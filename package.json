{
  "name": "@atlauncher/discord-bot",
  "version": "0.1.0",
  "description": "This is the code for our Discord bot which runs on our official Discord server at https://atl.pw/discord",
  "main": "dist",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ATLauncher/Discord-Bot.git"
  },
  "keywords": [
    "discord",
    "atlauncher",
    "bot"
  ],
  "author": "Ryan Dowling <ryan.dowling@atlauncher.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ATLauncher/Discord-Bot/issues"
  },
  "homepage": "https://github.com/ATLauncher/Discord-Bot#readme",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "config:base64": "ts-node utils/stringifyConfig.ts",
    "dev": "cross-env NODE_ENV=development nodemon",
    "oauth:url": "ts-node utils/getOAuthUrl.ts",
    "postinstall": "patch-package",
    "start": "node dist",
    "test": "echo 'Error: no tests written :( bad dev'"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,utils}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@koa/router": "^9.3.1",
    "@sentry/node": "^5.20.1",
    "config": "^3.3.1",
    "date-fns": "^2.15.0",
    "discord.js": "^12.2.0",
    "dotenv": "8.2.0",
    "got": "^11.5.1",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "nedb": "^1.8.0",
    "nedb-promises": "^4.0.4",
    "node-schedule": "^1.3.2",
    "pluralize": "^8.0.0",
    "winston": "^3.3.3",
    "winston-logzio": "^5.1.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^9.1.1",
    "@types/config": "0.0.36",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa__router": "^8.0.2",
    "@types/node": "^14.0.26",
    "@types/node-schedule": "^1.3.0",
    "@types/pluralize": "0.0.29",
    "@types/ws": "^7.2.6",
    "cross-env": "^7.0.2",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "patch-package": "^6.2.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
